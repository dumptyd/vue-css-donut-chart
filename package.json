{
  "name": "vue-css-donut-chart",
  "version": "2.1.0",
  "description": "Lightweight Vue component for creating donut charts",
  "author": "dumptyd <dumptyd2.0@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build:site": "npm run lint && npm run type-check && BUILD_TARGET=site vite build",
    "build:lib": "npm run lint && npm run type-check && BUILD_TARGET=lib vite build && cp dist/vcdonut.umd.js dist/vcdonut.umd.min.js",
    "preview": "vite preview",
    "test": "vitest --coverage",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "main": "dist/vcdonut.umd.js",
  "module": "dist/vcdonut.mjs",
  "unpkg": "dist/vcdonut.umd.js",
  "types": "dist/vcdonut.d.ts",
  "peerDependencies": {
    "vue": "^3"
  },
  "devDependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-istanbul": "^2.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "lodash-es": "^4.17.21",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vitest": "^2.0.5",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.11"
  },
  "bugs": "https://github.com/dumptyd/vue-css-donut-chart/issues",
  "homepage": "https://dumptyd.github.io/vue-css-donut-chart/",
  "keywords": [
    "donut",
    "pie",
    "chart",
    "circle",
    "radial",
    "progress",
    "vue",
    "css"
  ],
  "license": "MIT",
  "repository": "https://github.com/dumptyd/vue-css-donut-chart"
}
